<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
    <metadata>
        <id>CloudFoundry.Buildpack.V2</id>
        <version>0.0.0</version>
        <authors>Andrew Stakhov</authors>
        <owners>Andrew Stakhov</owners>
        <projectUrl>https://github.com/macsux/cf-buildpack-template</projectUrl>
        <license type="expression">MIT</license>
        <iconUrl>https://dtb5pzswcit1e.cloudfront.net/assets/images/product_logos/icon_buildpacks@2x.png</iconUrl>
